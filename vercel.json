{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/standalone",
  "headers": [
    {
      "source": "/mfe/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/mfe/:path*",
      "destination": "/mfe/:path*"
    }
  ]
}
